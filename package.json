{
  "name": "vscode-external",
  "displayName": "external",
  "description": "Run External Tools",
  "version": "0.0.1",
  "publisher": "axetroy",
  "engines": {
    "vscode": "^1.21.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axetroy/vscode-external.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axetroy/vscode-external/issues"
  },
  "homepage": "https://github.com/axetroy/vscode-external#readme",
  "keywords": ["external", "vscode", "extension"],
  "categories": ["Other"],
  "icon": "resources/icon.png",
  "main": "./out/extension",
  "activationEvents": ["*"],
  "contributes": {
    "commands": [
      {
        "command": "external.define",
        "title": "Define External Tool",
        "category": "External"
      },
      {
        "command": "external.run",
        "title": "Run External Tool",
        "category": "External"
      },
      {
        "command": "external.remove",
        "title": "Remove External Tool",
        "category": "External"
      }
    ],
    "configuration": {
      "title": "External",
      "properties": {
        "external.commands": {
          "description": "The command you have defined",
          "type": "array",
          "default": []
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run lint",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p ./ -c tslint.json",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^9.4.7",
    "conventional-changelog-cli": "^1.3.16",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1",
    "vscode": "^1.1.14"
  },
  "dependencies": {}
}
